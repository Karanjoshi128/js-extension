{
    "manifest_version": 3,
    "name": "History Manager",
    "version": "1.0",
    "description": "An extension to manage browsing history.",
    "permissions": ["history", "bookmarks"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  